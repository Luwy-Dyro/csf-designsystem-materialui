{
  "name": "@luwy-dyro/tokens",
  "version": "0.4.0",
  "main": "./tailwind-preset.js",
  "files": [
    "dist",
    "tokens.clean.json",
    "tailwind-preset.js"
  ],
  "scripts": {
    "build": "node transform-tokens.js && style-dictionary build --config sd.config.cjs && node scripts/build-fonts.cjs",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "style-dictionary": "^3.9.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luwy-Dyro/csf-designsystem-materialui.git",
    "directory": "packages/tokens"
  },
  "exports": {
    ".": {
      "types": "./dist/js/index.d.ts",
      "import": "./dist/js/index.js",
      "require": "./dist/js/index.js"
    },
    "./dist/css/variables.css": {
      "import": "./dist/css/variables.css",
      "require": "./dist/css/variables.css"
    },
    "./dist/css/fonts.css": "./dist/css/fonts.css",
    "./dist/css/theme-aliases.css": "./dist/css/theme-aliases.css",
    "./css/*": "./dist/css/*"
  }
}