{
  "name": "@csf/tokens",
  "version": "0.0.0",
  "main": "./dist/js/index.js",
  "types": "./dist/js/index.d.ts",
  "files": ["dist"],
  "scripts": {
  "build": "node transform-tokens.js && style-dictionary build --config sd.config.cjs && node scripts/build-fonts.cjs",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "style-dictionary": "^3.9.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "exports": {
      ".": {
          "types": "./dist/js/index.d.ts",
          "import": "./dist/js/index.js",
          "require": "./dist/js/index.js"
      },
      "./dist/css/variables.css": {
          "import": "./dist/css/variables.css",
          "require": "./dist/css/variables.css"
    },
    "./dist/css/fonts.css": {
      "import": "./dist/css/fonts.css",
      "require": "./dist/css/fonts.css"
    }
  }

}